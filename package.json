{
  "name": "@tadashi/svelte-loading",
  "version": "3.0.0",
  "description": "Svelte component",
  "author": "Thiago Lagden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lagden/svelte-loading/issues"
  },
  "homepage": "https://github.com/lagden/svelte-loading#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "svelte",
    "loading",
    "component"
  ],
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "type": "module",
  "svelte": "src/loading.js",
  "main": "src/loading.js",
  "scripts": {
    "prettier": "prettier -c --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "format": "prettier --write --plugin-search-dir=. ./src/**/*.{svelte,js}",
    "eslint": "eslint --config .eslintrc.cjs --ext .js --ignore-path .gitignore .",
    "rm": "rm -rf dist",
    "cp": "cp dist/* public/module/.",
    "esbuild": "node ./esbuild.config.js",
    "build": "run-s test rm esbuild cp",
    "prepublishOnly": "npm run build",
    "pretest": "npm run eslint",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:watch": "npm test -- --watch",
    "server": "sirv public --cors --single --host 0.0.0.0 --port 5000 --dev"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/svelte": "3.2.2",
    "esbuild": "0.15.10",
    "esbuild-svelte": "0.7.1",
    "eslint": "8.24.0",
    "eslint-config-xo": "0.42.0",
    "eslint-plugin-svelte3": "4.0.0",
    "eslint-plugin-unicorn": "44.0.1",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.1",
    "sirv-cli": "2.0.2",
    "svelte": "3.50.1",
    "svelte-jester": "2.3.2",
    "timekeeper": "2.2.0"
  },
  "packageManager": "yarn@3.2.4"
}
